# LingoFlow 环境配置示例
# 复制此文件为 .env 并填入实际值

# Database
DB_HOST=localhost
DB_PORT=3306
DB_NAME=newlingoflow
DB_USER=root
DB_PASSWORD=123456

# JWT
JWT_SECRET=your-256-bit-secret-key-here-must-be-at-least-64-characters-long-for-security
JWT_ACCESS_EXPIRATION=3600000
JWT_REFRESH_EXPIRATION=604800000

# Server
SERVER_PORT=8080
